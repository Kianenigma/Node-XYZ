{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
      "include": ["./xyz.js", "./src"],
      "includePattern": ".+\\.js(doc)?$",
	    "exclude": ["./src/Util/Util.js", "node_modules", "./examples"],
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
    "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "template": "node_modules/docdash"
    }
}
